{
  "name": "neode",
  "version": "0.3.3",
  "description": "Neo4j OGM for NodeJS",
  "main": "build/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "./node_modules/.bin/babel src -d build",
    "test": "./node_modules/.bin/mocha --require babel-core/register ./test{/,/**/}*.spec.js",
    "lint": "./node_modules/.bin/eslint src/",
    "coverage": "./node_modules/.bin/nyc --reporter=html npm test",
    "tdd": "./node_modules/.bin/mocha --require babel-core/register --watch"
  },
  "keywords": [
    "neo4j",
    "graph",
    "cypher",
    "ogm"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adam-cowley/neode.git"
  },
  "author": "Adam Cowley <github@adamcowley.co.uk>",
  "license": "MIT",
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "dotenv": "^4.0.0",
    "neo4j-driver": "^1.7.5",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "clear": "0.0.1",
    "eslint": "^4.19.1",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "bugs": {
    "url": "https://github.com/adam-cowley/neode/issues"
  },
  "homepage": "https://github.com/adam-cowley/neode#readme"
}
